{
  "name": "simple-subway",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev src/index.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig-build.json",
    "start:prod": "node dist/index.js",
    "migration:run": "ts-node --transpile-only -e 'import { MySQLMigration } from \"./src/database/mysql/migration.ts\"; new MySQLMigration().run()'",
    "migration:run:test": "NODE_ENV=test npm run migration:run",
    "migration:create": "ts-node --transpile-only -e 'import { MySQLMigration } from \"./src/database/mysql/migration.ts\"; new MySQLMigration().create()'",
    "test": "vitest --run ",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:staged": "vitest related --run ",
    "test:unit": "vitest -r src .spec.ts --run",
    "test:integration": "vitest -r src .test.ts --run",
    "test:e2e": "vitest -r test/e2e --run",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "pactum": "^3.4.0",
    "pactum-matchers": "^1.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "fastify": "^4.17.0",
    "module-alias": "^2.2.2",
    "mongodb": "^5.5.0",
    "mysql2": "^3.3.0",
    "yup": "^1.1.1"
  },
  "_moduleAliases": {
    "#card": "./dist/@core/card",
    "#station": "./dist/@core/station",
    "#shared": "./dist/@core/@shared",
    "#core": "./dist/@core",
    "#api": "./dist/api"
  }
}
